{
  "name": "codecall",
  "displayName": "codecall",
  "description": "Your coding companion with a beautiful UI",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:codecall.sidebarView",
    "onCommand:codecall.helloWorld",
    "onCommand:codecall.spawnAgent",
    "onCommand:codecall.dismissAllAgents"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codecall.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "codecall.spawnAgent",
        "title": "Codecall: Spawn Agent"
      },
      {
        "command": "codecall.dismissAllAgents",
        "title": "Codecall: Dismiss All Agents"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codecall-sidebar",
          "title": "Codecall",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "codecall-sidebar": [
        {
          "type": "webview",
          "id": "codecall.sidebarView",
          "name": "Codecall"
        }
      ]
    },
    "configuration": {
      "title": "Codecall",
      "properties": {
        "codecall.cursorApiKey": {
          "type": "string",
          "default": "",
          "description": "API key for Cursor agent CLI"
        },
        "codecall.elevenLabsApiKey": {
          "type": "string",
          "default": "",
          "description": "API key for ElevenLabs voice services"
        },
        "codecall.elevenLabsAgentId": {
          "type": "string",
          "default": "",
          "description": "ElevenLabs Conversational AI Agent ID (optional)"
        },
        "codecall.defaultVoicePreset": {
          "type": "string",
          "enum": [
            "professional",
            "friendly",
            "technical",
            "calm",
            "energetic"
          ],
          "default": "professional",
          "description": "Default voice preset for new voice agents"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && node esbuild.js --production",
    "compile:extension": "tsc -p ./",
    "compile:webviews": "node esbuild.js",
    "watch": "npm run compile:webviews && concurrently \"tsc -watch -p ./\" \"node esbuild.js --watch\"",
    "dev": "./dev.sh",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/elevenlabs": "^2.0.15",
    "@ai-sdk/openai": "^3.0.23",
    "@ai-sdk/react": "^3.0.64",
    "@elevenlabs/client": "^0.13.1",
    "@elevenlabs/elevenlabs-js": "^2.33.0",
