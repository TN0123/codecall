# Codecall Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# API Keys
# =============================================================================

# OpenAI API key (required for AI agent and Vercel AI SDK)
OPENAI_API_KEY=your_openai_api_key_here

# Cursor CLI API key (required for spawning Cursor agents)
CURSOR_API_KEY=your_cursor_api_key_here

# ElevenLabs API key (required for voice features - TTS and STT)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# =============================================================================
# ElevenLabs Configuration (optional)
# =============================================================================

# ElevenLabs Conversational AI Agent ID (optional, for voice conversations)
ELEVENLABS_AGENT_ID=your_agent_id_here

# =============================================================================
# Server Configuration (optional)
# =============================================================================

# Port for the local server (default: 3000)
PORT=3000
